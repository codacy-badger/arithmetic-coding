{
  "name": "arithmetic-coding",
  "version": "1.1.0",
  "description": "Arithmetic coding implementation",
  "main": "src/index.js",
  "bin": {
    "ari-coding": "bin/index.js"
  },
  "scripts": {
    "test": "mocha --timeout 10000 && npm run clean",
    "clean": "rm test/txt/*-encoded.txt test/txt/*-decoded.txt",
    "cover": "istanbul cover ./node_modules/mocha/bin/_mocha",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls",
    "patch": "npm version patch && git push origin master && git push origin --tags",
    "minor": "npm version minor && git push origin master && git push origin --tags",
    "major": "npm version major && git push origin master && git push origin --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/upupming/arithmetic-coding.git"
  },
  "keywords": [
    "arithmetic coding",
    "encoding",
    "lossless",
    "compression"
  ],
  "author": "upupming",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/upupming/arithmetic-coding/issues"
  },
  "homepage": "https://github.com/upupming/arithmetic-coding#readme",
  "dependencies": {
    "chalk": "^2.4.2",
    "commander": "^2.19.0",
    "concat-stream": "^2.0.0",
    "long": "^4.0.0",
    "which": "^1.3.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "chai": "^4.2.0",
    "coveralls": "^3.0.3",
    "istanbul": "^0.4.5",
    "mocha": "^6.0.2",
    "should": "^13.2.3"
  }
}
